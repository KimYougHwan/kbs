		</if>
		<if test = "chargeYn != null and chargeYn != ''">
			charge_yn= #{chargeYn} ,
		</if>
		<if test = "amount != null and amount != ''">
			amount= #{amount} ,
		</if>
		<if test = "totalAmount != null and totalAmount != ''">
			total_amount= #{totalAmount} ,
		</if>
		<if test = "chargeCnt != null and chargeCnt != ''">
