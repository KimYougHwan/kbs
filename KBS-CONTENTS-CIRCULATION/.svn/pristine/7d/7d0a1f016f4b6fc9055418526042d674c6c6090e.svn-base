		<if test = "chargeVtime != null and chargeVtime != ''">
			charge_vtime= #{chargeVtime} ,
		</if>
		<if test = "hChargeVtime != null and hChargeVtime != ''">
			h_charge_vtime= #{hChargeVtime} ,
